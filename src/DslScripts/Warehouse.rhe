Module Warehouse1
{
    Entity Warehouse
    {
        Shortstring Name;
        DateTime CreatedAt { CreationTime; }
    }

    // Goods that can be stored in the warehouse.
    Entity WarehouseItem
    {
        ShortString Name;
    }

    // User-friendly display of the current quantity of each item for each warehouse.
    Browse WarehouseCurrentStateGrid Warehouse1.WarehouseCurrentState
    {
        Take 'Warehouse.Name';
        Take 'WarehouseItem.Name';
        Take Quantity; 
    }
}